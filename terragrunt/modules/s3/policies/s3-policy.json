{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "${role_arn}",
          "arn:aws:iam::590183659157:role/terraform_role"
        ]
      },
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::${environment}-example-bucket",
        "arn:aws:s3:::${environment}-example-bucket/*"
      ]
    }
  ]
}
